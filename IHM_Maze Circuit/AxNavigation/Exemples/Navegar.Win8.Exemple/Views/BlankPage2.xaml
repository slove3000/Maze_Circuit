<Page
    x:Class="Navegar.Win8.Exemple.Views.BlankPage2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    DataContext="{Binding BlankPage2ViewModel, Source={StaticResource Locator}}">

    <Grid Background="{StaticResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock Text="{Binding ReceiveFromPage}" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20" />
        <Button Content="Revenir Page 1" Command="{Binding GoBackCommand}" />
    </Grid>
</Page>